    function generateAIOutput() {
      // Replace this with your AI generation logic
      // You can use external libraries or APIs to generate AI content

      // For demonstration purposes, let's generate a random number
      var randomNumber = Math.floor(Math.random() * 100);

      // Update the output element with the generated content
      document.getElementById('output').textContent = "Generated AI Output: " + randomNumber;
    }

    document.getElementById('generate-btn').addEventListener('click', generateAIOutput);
